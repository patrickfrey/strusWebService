{
	"data" :
	{
		"workdir"   : "/srv/strus",
		"configdir" : "/srv/strus/config",
		"resources" : "/usr/share/strus"
	},

	"transactions" : {
		"max_idle_time" : 600
	},

	"extensions" : {
		"directory" : "@STRUS_MODULE_PATH@",
		"modules" : [
			"modstrus_analyzer_pattern",
			"modstrus_storage_vector_std"
		]
	},

	"http" : {
		"script" : "/strus"
	},

	"service" : {
		"api" : "http",
		"ip" : "0.0.0.0",
		"port" : 8080,
	},

	"security" : {
		"content_length_limit": 100000,
		"file_in_memory_limit": 100000,
		"cors" : {
			"enable" : true,
			"allowed_origins" : [
				"http://localhost:1313"
			],
			"age" : 10
		}
	},

	"logging" : {
		"level" : "debug",
		"file" : {
			"name":		"./strusWebService.log",
			"append":	true
		}
	},

	"debug" : {
		"log_requests": true,
		"struct_depth": 4,
		"request_file": "./requests.log",
		"protocol" : {
			"pretty_print":		true,
			"quit_enabled":		true,
			"debug_enabled":	true
		}
	}
}


